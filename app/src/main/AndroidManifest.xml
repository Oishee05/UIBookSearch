<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uibooksearch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AddSellerLocation"
            android:label="@string/title_activity_add_seller_location"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".SellerInfo"
            android:label="@string/title_activity_seller_info"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DisplaySellerLocation"
            android:label="@string/title_activity_display_seller_location"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BuyerInfo"
            android:label="@string/title_activity_buyer_info"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>